	<para id="2006">
		<se lang="kor" has_translit="true">
			<w translit="nae">
				<ana lex="내" gr="S" transl="я, мой, моя, мое; речка, ручей; запах, аромат" lex_translit="nae">내</ana>
			</w>
			<w translit="gachi">
				<ana lex="가치" gr="S" transl="ценность" lex_translit="gachi">가치</ana>
			</w>
			<w translit="reul">
				<ana lex="를" gr="PART,acc" transl="" lex_translit="reul">를</ana>
			</w>
			<w translit="kiwo">
				<ana lex="키우다" gr="V" transl="выращивать, воспитывать" lex_translit="kiuda">키워</ana>
			</w>
		</se>
