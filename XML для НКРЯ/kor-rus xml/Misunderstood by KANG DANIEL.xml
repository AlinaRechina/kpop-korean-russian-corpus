<?xml version='1.0' encoding='utf-8'?>
<xml>
	<para id="1002">
		<se lang="kor" has_translit="true">
			<w translit="nan">
				<ana lex="난" gr="S" transl="столбец, колонка; бунт, мятеж" lex_translit="nan">난</ana>
			</w>
			<w translit="eodum">
				<ana lex="어둠" gr="S" transl="темнота" lex_translit="eodum">어둠</ana>
			</w>
			<w translit="sok">
				<ana lex="속" gr="S" transl="внутренняя часть, содержание, намерение" lex_translit="sok">속</ana>
			</w>
			<w translit="e">
				<ana lex="에" gr="PART" transl="" lex_translit="e">에</ana>
			</w>
			<w translit="gathyeojin">
				<ana lex="갇히다" gr="V,partcp" transl="; быть в заточении" lex_translit="gathida">갇혀진</ana>
			</w>
			<w translit="deuthae">
				<ana lex="듯해다" gr="A,aux" transl="" lex_translit="deuthaeda">듯해</ana>
			</w>
		</se>
		<se lang="rus">У меня такое чувство, будто я застрял в темноте.
		</se>
	</para>
	<para id="1003">
		<se lang="kor" has_translit="true">
			<w translit="eotteon">
				<ana lex="어떤다" gr="A" transl="" lex_translit="eotteonda">어떤</ana>
			</w>
			<w translit="maldeuldo">
				<ana lex="말" gr="S" transl="лошадь, конь; слово, речь, язык; конец" lex_translit="mal">말들도</ana>
			</w>
			<w translit="uimi">
				<ana lex="의미" gr="S" transl="значение" lex_translit="uimi">의미</ana>
			</w>
			<w translit="eopneunde">
				<ana lex="없" gr="A,ger" transl="" lex_translit="eop">없는데</ana>
			</w>
		</se>
		<se lang="rus">Некоторые слова ничего не значат.
		</se>
	</para>
	<para id="2004">
		<se lang="kor" has_translit="true">
			<w translit="daeul">
				<ana lex="닿다" gr="V,partcp" transl="соприкасаться, доставать" lex_translit="data">닿을</ana>
			</w>
			<w translit="ttaemyeon">
				<ana lex="때다" gr="V,ger" transl="топить, разжигать, паять, сварить" lex_translit="ttaeda">때면</ana>
			</w>
			<w translit="sum">
				<ana lex="숨" gr="S" transl="дыхание, свежесть овощей" lex_translit="sum">숨</ana>
			</w>
			<w translit="i">
				<ana lex="이" gr="PART" transl="" lex_translit="i">이</ana>
			</w>
			<w translit="makhyeowa">
				<ana lex="막히다" gr="V" transl="быть закрытым, закупоренным, прегражденным" lex_translit="makhida">막혀와</ana>
			</w>
		</se>
		<se lang="rus">Когда ты доходишь до нее, то начинаешь задыхаться.
		</se>
	</para>
	<para id="3001">
		<se lang="kor" has_translit="true">
			<w translit="pulriji">
				<ana lex="풀리다" gr="V,ger" transl="развязываться, распутываться" lex_translit="pulrida">풀리지</ana>
			</w>
			<w translit="ana">
				<ana lex="않다" gr="V,ger" transl="не" lex_translit="anta">않아</ana>
			</w>
			<w translit="eongkyeobeorin">
				<ana lex="엉키다" gr="V,partcp" transl="переплетаться; запутываться; свёртываться; смешиваться; сворачиваться; запутаться; смешаться; запутаться в чувствах; перепутываться; спутываться; сцепляться; перемешиваться" lex_translit="eongkida">엉켜버린</ana>
			</w>
			<w translit="bam">
				<ana lex="밤" gr="S" transl="ночь; каштан" lex_translit="bam">밤</ana>
			</w>
		</se>
		<se lang="rus">Он не потерян, но ночь слишком запутана.
		</se>
	</para>
	<para id="3002">
		<se lang="kor" has_translit="true">
			<w translit="">
				<ana lex="" gr="" transl="" lex_translit="">공허</ana>
			</w>
			<w translit="haejyeoga">
				<ana lex="해지다" gr="V" transl="трепаться; рваться; очищаться; чиститься; очищаться; осветляться; становиться ясным; становиться опрятным; становиться аккуратным; становиться чистым; чистый; освежающий; бодрящий (вкус); очищаться; исчезать бесследно; без последствий; становиться прямым; становиться несгибаемым; становиться правильным; становиться простым; становиться чистым; привыкать; приспосабливаться" lex_translit="haejida">해져가</ana>
			</w>
			<w translit="jinsimiran">
				<ana lex="진심이란" gr="S" transl="" lex_translit="jinsimiran">진심이란</ana>
			</w>
			<w translit="mal">
				<ana lex="말" gr="S" transl="лошадь, конь; слово, речь, язык; конец" lex_translit="mal">말</ana>
			</w>
		</se>
		<se lang="rus">Это я имею в виду, имею в виду, имею в виду.
		</se>
	</para>
	<para id="3003">
		<se lang="kor" has_translit="true">
			<w translit="gwaenchantago">
				<ana lex="괜찮다" gr="A" transl="нормально, ничего, неплохой, сносный" lex_translit="gwaenchanta">괜찮다고</ana>
			</w>
			<w translit="doenoeyeobojiman">
				<ana lex="되뇌이다" gr="V,ger" transl="; " lex_translit="doenoeida">되뇌여보지만</ana>
			</w>
		</se>
		<se lang="rus">Думаю, все в порядке.
		</se>
	</para>
	<para id="5005">
		<se lang="kor" has_translit="true">
			<w translit="naega">
				<ana lex="내가다" gr="V" transl="вывозить, выносить" lex_translit="naegada">내가</ana>
			</w>
			<w translit="mueot">
				<ana lex="무엇" gr="S" transl="что, что-то, что-нибудь" lex_translit="mueot">무엇</ana>
			</w>
			<w translit="eul">
				<ana lex="을" gr="PART,acc" transl="" lex_translit="eul">을</ana>
			</w>
			<w translit="eojjae">
				<ana lex="어째" gr="ADV" transl="как; отчего; почему; как; почему; отчего; случайно; ненамеренно; нечаянно; иногда; время от времени; порой" lex_translit="eojjae">어째</ana>
			</w>
		</se>
		<se lang="rus">Что я делаю?
		</se>
	</para>
	<para id="5006">
		<se lang="kor" has_translit="true">
			<w translit="sumgo">
				<ana lex="숨다" gr="V,ger" transl="прятаться, скрываться" lex_translit="sumda">숨고</ana>
			</w>
			<w translit="sipeun">
				<ana lex="싶다" gr="V,partcp" transl="хотеть; желать; казаться; быть похожим на что-либо; как бы ни; хотелось бы, чтобы... ; лишь бы было так; захотеть, чтобы; кажется; думать; думать, может; кажется; хотеть (что-либо делать); думать; кажется; вероятно; кажется; видимо; кажется" lex_translit="sipda">싶은</ana>
			</w>
			<w translit="geon">
				<ana lex="건" gr="S" transl="дело, событие" lex_translit="geon">건</ana>
			</w>
			<w translit="dangyeonhan">
				<ana lex="당연다" gr="V,partcp" transl="" lex_translit="dangyeonda">당연한</ana>
			</w>
			<w translit="geonde">
				<ana lex="것다" gr="V,ger" transl="" lex_translit="geotda">건데</ana>
			</w>
		</se>
		<se lang="rus">Неудивительно, что я хочу спрятаться.
		</se>
	</para>
	<para id="6001">
		<se lang="kor" has_translit="true">
			<w translit="jiteun">
				<ana lex="짙다" gr="A,partcp" transl="яркий; чёткий; отчётливый; чёткий; отчётливый; чёрный; крепкий; густой; насыщенный; сильный; резкий; сильный; усиленный" lex_translit="jitda">짙은</ana>
			</w>
			<w translit="angae">
				<ana lex="안개" gr="S" transl="туман" lex_translit="angae">안개</ana>
			</w>
			<w translit="ga">
				<ana lex="가" gr="PART" transl="" lex_translit="ga">가</ana>
			</w>
			<w translit="kkalrineun">
				<ana lex="깔리다" gr="V,partcp" transl="быть подстеленным; быть разостланным; выставиться; разложиться; задолжать(ся); утонуть в долгах; (быть) в долгах, как в шелках; быть задавленным; быть задавленным; быть под колпаком; быть под (чьим-либо) каблуком; распространяться; расстилаться; подразумеваться; наступить; заполонить (о темноте или тяжёлой атмосфере); глухой голос; приглушённый голос; садиться; покрывать" lex_translit="kkalrida">깔리는</ana>
			</w>
			<w translit="deuthae">
				<ana lex="듯해다" gr="A,aux" transl="" lex_translit="deuthaeda">듯해</ana>
			</w>
		</se>
		<se lang="rus">Это как густой туман.
		</se>
	</para>
	<para id="6002">
		<se lang="kor" has_translit="true">
			<w translit="jageun">
				<ana lex="작다" gr="A,partcp" transl="маленький, небольшой" lex_translit="jakda">작은</ana>
			</w>
			<w translit="soksagim">
				<ana lex="속삭임" gr="S" transl="шёпот; шуршание" lex_translit="soksagim">속삭임</ana>
			</w>
			<w translit="nareul">
				<ana lex="나르다" gr="V,partcp" transl="возить, носить, переносить" lex_translit="nareuda">나를</ana>
			</w>
			<w translit="hwigama">
				<ana lex="휘감다" gr="V,ger" transl="Окутывать; обматывать; Обвивать; Окружать; обтекать; Завивать; закатать; сворачивать; сворачивать калачиком; Охватывать; покрывать; объять; Выряжаться; наряжаться" lex_translit="hwigamda">휘감아</ana>
			</w>
		</se>
		<se lang="rus">Легкий шепот проносится надо мной,
		</se>
	</para>
	<para id="6003">
		<se lang="kor" has_translit="true">
			<w translit="kkeuneonael">
				<ana lex="끊다" gr="V,ger" transl="разрывать, прекращать" lex_translit="kkeunta">끊어낼</ana>
			</w>
			<w translit="sujocha">
				<ana lex="수조차" gr="S,aux" transl="" lex_translit="sujocha">수조차</ana>
			</w>
			<w translit="eopneun">
				<ana lex="없다" gr="A,partcp" transl="не быть, не иметь, отсутствовать" lex_translit="eopda">없는</ana>
			</w>
			<w translit="naege">
				<ana lex="내" gr="ADV" transl="я, мой, моя, мое; речка, ручей; запах, аромат" lex_translit="nae">내게</ana>
			</w>
		</se>
		<se lang="rus">Я даже не могу убежать от него.
		</se>
	</para>
	<para id="7004">
		<se lang="kor" has_translit="true">
			<w translit="gamanhi">
				<ana lex="가만히" gr="ADV" transl="спокойно; тихо; безмолвно; неподвижно; бесшумно; безучастно; равнодушно; как есть; спокойно; внимательно; хладнокровно; тихо; украдкой; тихонько; незаметно; тихо-тихо; украдкой; потихоньку; тайком" lex_translit="gamanhi">가만히</ana>
			</w>
			<w translit="nareul">
				<ana lex="나르다" gr="V,partcp" transl="возить, носить, переносить" lex_translit="nareuda">나를</ana>
			</w>
			<w translit="biwonae">
				<ana lex="비우다" gr="V" transl="опорожнять, освобождать" lex_translit="biuda">비워내</ana>
			</w>
		</se>
		<se lang="rus">Держишь меня опустошенным.
		</se>
	</para>
	<para id="9006">
		<se lang="kor" has_translit="true">
			<w translit="maemdora">
				<ana lex="맴돌다" gr="V,ger" transl="кружиться на одном месте; вертеться; вращаться вокруг своей оси; топтаться на месте; вертеться (о мыслях, чувствах и т.п.)" lex_translit="maemdolda">맴돌아</ana>
			</w>
			<w translit="nal">
				<ana lex="날" gr="S" transl="день, сутки, лезвие, острие, сырой, незрелый" lex_translit="nal">날</ana>
			</w>
		</se>
		<se lang="rus">День проходит слишком шумно.
		</se>
	</para>
	<para id="9008">
		<se lang="kor" has_translit="true">
			<w translit="michil">
				<ana lex="미치다" gr="V,partcp" transl="сходить с ума, бесноваться; достигать, доходить" lex_translit="michida">미칠</ana>
			</w>
			<w translit="deuthae">
				<ana lex="듯해다" gr="A,aux" transl="" lex_translit="deuthaeda">듯해</ana>
			</w>
		</se>
		<se lang="rus">Это безумие
		</se>
	</para></xml>