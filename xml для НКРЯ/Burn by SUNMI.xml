			</w>
			<w translit="eul">
				<ana lex="을" gr="PART,acc" transl="" lex_translit="eul">을</ana>
			</w>
			<w translit="nan">
				<ana lex="난" gr="S" transl="столбец, колонка; бунт, мятеж" lex_translit="nan">난</ana>
			</w>
			<w translit="badasseo">
				<ana lex="받다" gr="V" transl="получать" lex_translit="batda">받았어</ana>
			</w>
		</se>
