<?xml version='1.0' encoding='utf-8'?>
<xml>
	<para id="1001">
		<se lang="kor" has_translit="true">
			<w translit="neon">
				<ana lex="널다" gr="V,partcp" transl="развешивать, вешать" lex_translit="neolda">넌</ana>
			</w>
			<w translit="ga">
				<ana lex="가" gr="PART" transl="" lex_translit="ga">가</ana>
			</w>
			<w translit="gunggeumhae">
				<ana lex="궁금하다" gr="A" transl="волноваться, хотеть знать" lex_translit="gunggeumhada">궁금해</ana>
			</w>
		</se>
		<se lang="rus">О чем ты переживаешь?
		</se>
	</para>
	<para id="2001">
		<se lang="kor" has_translit="true">
			<w translit="siganeun">
				<ana lex="시간은" gr="S" transl="" lex_translit="siganeun">시간은</ana>
			</w>
			<w translit="chungbunhae">
				<ana lex="충분다" gr="V" transl="" lex_translit="chungbunda">충분해</ana>
			</w>
			<w translit="jigeum">
				<ana lex="지금" gr="S" transl="сейчас; теперь; настоящее (время); сейчас; сейчас; в это время; в настоящем (времени); теперь; до сих пор" lex_translit="jigeum">지금</ana>
			</w>
			<w translit="uri">
				<ana lex="우리" gr="PRO" transl="мы" lex_translit="uri">우리</ana>
			</w>
		</se>
		<se lang="rus">У нас сейчас достаточно времени,
		</se>
	</para>
	<para id="2004">
		<se lang="kor" has_translit="true">
			<w translit="saljjakman">
				<ana lex="살짝만" gr="ADV" transl="" lex_translit="saljjakman">살짝만</ana>
			</w>
			<w translit="seuchyeodo">
				<ana lex="스치다" gr="V,ger" transl="задеть; едва коснуться; дотронуться; пронестись (о запахе, ветре, звуке); промелькнуть; проскользнуть; проскользнуть" lex_translit="seuchida">스쳐도</ana>
			</w>
			<w translit="jeongsineopji">
				<ana lex="정신없" gr="A,ger" transl="" lex_translit="jeongsineop">정신없지</ana>
			</w>
		</se>
		<se lang="rus">Даже просто аромат сводит тебя с ума.
		</se>
	</para>
	<para id="3001">
		<se lang="kor" has_translit="true">
			<w translit="seoltang">
				<ana lex="설탕" gr="S" transl="сахарный песок" lex_translit="seoltang">설탕</ana>
			</w>
			<w translit="i">
				<ana lex="이" gr="PART" transl="" lex_translit="i">이</ana>
			</w>
			<w translit="balrin">
				<ana lex="발린" gr="S" transl="" lex_translit="balrin">발린</ana>
			</w>
			<w translit="mat">
				<ana lex="맛" gr="S" transl="вкус, настроение, атмосфера" lex_translit="mat">맛</ana>
			</w>
			<w translit="e">
				<ana lex="에" gr="PART" transl="" lex_translit="e">에</ana>
			</w>
			<w translit="neun">
				<ana lex="는" gr="PART" transl="" lex_translit="neun">는</ana>
			</w>
		</se>
		<se lang="rus">Так просто сойти с ума от одного тогда сладковатого вкуса,
		</se>
	</para>
	<para id="3002">
		<se lang="kor" has_translit="true">
			<w translit="inae">
				<ana lex="이내" gr="ADV" transl="в пределах; тот час; с тех пор; динамичный" lex_translit="inae">이내</ana>
			</w>
			<w translit="jilrigi">
				<ana lex="질리" gr="S" transl="разочароваться (в ком-либо); терять веру; надоедать; наскучить; побелеть; вывихнуть" lex_translit="jilri">질리기</ana>
			</w>
			<w translit="swiwo">
				<ana lex="쉽다" gr="A" transl="легкий, простой; возможно, вероятно" lex_translit="swipda">쉬워</ana>
			</w>
		</se>
		<se lang="rus">Ты понимаешь мой пылающие чувства в этот момент,
		</se>
	</para>
	<para id="3004">
		<se lang="kor" has_translit="true">
			<w translit="jigeum">
				<ana lex="지금" gr="S" transl="сейчас; теперь; настоящее (время); сейчас; сейчас; в это время; в настоящем (времени); теперь; до сих пор" lex_translit="jigeum">지금</ana>
			</w>
			<w translit="neukkyeotji">
				<ana lex="느끼다" gr="V" transl="чувствовать, ощущать, испытывать" lex_translit="neukkida">느꼈지</ana>
			</w>
			<w translit="neon">
				<ana lex="널다" gr="V,partcp" transl="развешивать, вешать" lex_translit="neolda">넌</ana>
			</w>
		</se>
		<se lang="rus">Я пробуждаю 5 чувствло,
		</se>
	</para>
	<para id="3005">
		<se lang="kor" has_translit="true">
			<w translit="neowa">
				<ana lex="너와" gr="S" transl="нова; новаджип; " lex_translit="neowa">너와</ana>
			</w>
			<w translit="nae">
				<ana lex="내" gr="S" transl="я, мой, моя, мое; речка, ручей; запах, аромат" lex_translit="nae">내</ana>
			</w>
			<w translit="sai">
				<ana lex="사이" gr="S" transl="между" lex_translit="sai">사이</ana>
			</w>
			<w translit="seonmyeonghi">
				<ana lex="선명히" gr="ADV" transl="; ясно; отчётливо; чётко" lex_translit="seonmyeonghi">선명히</ana>
			</w>
			<w translit="boin">
				<ana lex="보이다" gr="V,partcp" transl="видно, виднеться" lex_translit="boida">보인</ana>
			</w>
		</se>
		<se lang="rus">Я пробуждаю твои чувства,
		</se>
	</para>
	<para id="3006">
		<se lang="kor" has_translit="true">
			<w translit="keojyeoman">
				<ana lex="커지다" gr="V" transl="; увеличиваться; усиляться" lex_translit="keojida">커져만</ana>
			</w>
			<w translit="ganeun">
				<ana lex="갈다" gr="V,partcp" transl="менять, заменять; точить" lex_translit="galda">가는</ana>
			</w>
			<w translit="yeolgi">
				<ana lex="열기" gr="S" transl="горячий воздух; жар" lex_translit="yeolgi">열기</ana>
			</w>
		</se>
		<se lang="rus">Кажется, мой скил вот вот всё подорвет.
		</se>
	</para>
	<para id="5001">
		<se lang="kor" has_translit="true">
			<w translit="aye">
				<ana lex="아예" gr="ADV" transl="с нуля; заранее; ни в коем случае; никоим образом; полностью" lex_translit="aye">아예</ana>
			</w>
			<w translit="bangsim">
				<ana lex="방심" gr="S" transl="беспечность; беззаботность; неосторожность; невнимательность; халатность; быть неосторожным; быть невнимательным; относиться халатно" lex_translit="bangsim">방심</ana>
			</w>
			<w translit="hajima">
				<ana lex="하다" gr="V,ger" transl="делать" lex_translit="hada">하지마</ana>
			</w>
		</se>
		<se lang="rus">Совершенно не даю тебе расслабиться,
		</se>
	</para>
	<para id="6004">
		<se lang="kor" has_translit="true">
			<w translit="juche">
				<ana lex="주체" gr="S" transl="глава; голова; сердце; основа; базис; основание; канва; субъект; подлежащее; предмет; независимость; автономный; независимый; автономный; независимый" lex_translit="juche">주체</ana>
			</w>
			<w translit="mot">
				<ana lex="못" gr="S" transl="гвоздь; водоем, пруд; мозоль" lex_translit="mot">못</ana>
			</w>
			<w translit="hae">
				<ana lex="해" gr="S" transl="солнце; год; море; ущерб, вред" lex_translit="hae">해</ana>
			</w>
			<w translit="jakku">
				<ana lex="자꾸" gr="ADV" transl="непрерывно, часто, все время" lex_translit="jakku">자꾸</ana>
			</w>
			<w translit="ppajyeodeuneun">
				<ana lex="빠져들다" gr="V,partcp" transl="впасть; впасть; впадать; быть очарованным чем-либо" lex_translit="ppajyeodeulda">빠져드는</ana>
			</w>
			<w translit="mat">
				<ana lex="맛" gr="S" transl="вкус, настроение, атмосфера" lex_translit="mat">맛</ana>
			</w>
		</se>
		<se lang="rus">Ведь ты можешь сопротивляться, пока в конце концов я не заполучу тебя.
		</se>
	</para>
	<para id="7003">
		<se lang="kor" has_translit="true">
			<w translit="jungdokdwae">
				<ana lex="중독다" gr="V" transl="" lex_translit="jungdokda">중독돼</ana>
			</w>
			<w translit="imi">
				<ana lex="이미" gr="ADV" transl="уже; образ" lex_translit="imi">이미</ana>
			</w>
			<w translit="neon">
				<ana lex="널다" gr="V,partcp" transl="развешивать, вешать" lex_translit="neolda">넌</ana>
			</w>
		</se>
		<se lang="rus">Взбудораж свои чувства ещё больше(теперь уже никаких сомнений),
		</se>
	</para>
	<para id="7004">
		<se lang="kor" has_translit="true">
			<w translit="ne">
				<ana lex="네" gr="ANUM" transl="да; ты; четыре" lex_translit="ne">네</ana>
			</w>
			<w translit="mam">
				<ana lex="맘" gr="S" transl="душа; характер; сердце; душа; сердце; чувство; настроение; желание; симпатия; желание; намерение; стремление; Сколько душе угодно; по своему желанию; принять (твёрдое) решение; нрав; характер; душа; натура; душа" lex_translit="mam">맘</ana>
			</w>
			<w translit="eul">
				<ana lex="을" gr="PART,acc" transl="" lex_translit="eul">을</ana>
			</w>
			<w translit="deo">
				<ana lex="더" gr="S" transl="еще, более" lex_translit="deo">더</ana>
			</w>
			<w translit="seokkeonwa">
				<ana lex="섞다" gr="V,ger" transl="смешивать" lex_translit="seokda">섞어놔</ana>
			</w>
		</se>
		<se lang="rus">Ты продолжаешь хотеть его.
		</se>
	</para>
	<para id="7005">
		<se lang="kor" has_translit="true">
			<w translit="ijen">
				<ana lex="이젠" gr="S" transl="" lex_translit="ijen">이젠</ana>
			</w>
			<w translit="namgimeopsi">
				<ana lex="남김없이" gr="ADV" transl="без остатка, полностью" lex_translit="namgimeopsi">남김없이</ana>
			</w>
		</se>
		<se lang="rus">У тебя кружится голова, твои руки и ноги уже сдались на милость победителя,
		</se>
	</para>
	<para id="7006">
		<se lang="kor" has_translit="true">
			<w translit="gyesok">
				<ana lex="계속" gr="ADV" transl="продолжение" lex_translit="gyesok">계속</ana>
			</w>
			<w translit="wonhage">
				<ana lex="원하" gr="ADV" transl="желать что-либо; хотеть что-либо; прохладный; освежающий; приятно горячий; просторный; широкий; оживлённый; бодрый; свежий; чистый; удовлетворённый; облегчённый; свободный; удовлетворённый; приятный; сокращать людей; открывать; держаться почтительно, но на расстоянии; относиться с уважением, но холодно; Спасать; Избавлять от грехов; иметь своим источником что-либо; вести начало от чего-либо; восходить к чему-либо; происходить от чего-либо; исходить от чего-либо; молить; молиться; просить; умолять; желать" lex_translit="wonha">원하게</ana>
			</w>
			<w translit="dwae">
				<ana lex="되다" gr="A" transl="становиться, превращаться" lex_translit="doeda">돼</ana>
			</w>
		</se>
		<se lang="rus">Каждый простимулированый момент, так ярок,
		</se>
	</para>
	<para id="7007">
		<se lang="kor" has_translit="true">
			<w translit="eojireon">
				<ana lex="어지럽다" gr="A,partcp" transl="кружиться" lex_translit="eojireopda">어지런</ana>
			</w>
			<w translit="meori">
				<ana lex="머리" gr="S" transl="голова, волосы, ум, сознание, рассудок; верхушка, макушка, вершина; передняя часть, начало" lex_translit="meori">머리</ana>
			</w>
			<w translit="ne">
				<ana lex="네" gr="ANUM" transl="да; ты; четыре" lex_translit="ne">네</ana>
			</w>
			<w translit="du">
				<ana lex="두" gr="S" transl="два; толстый; плотный; крупный; большой; класть; оставлять; оставлять; подмешивать; добавлять; набивать; устраивать; собирать; организовывать; образовывать; учреждать; сооружать; акцентировать; подчеркивать; иметь в виду; тофу; два-три; головная боль; толщина; головной мозг; ум; сознание; ум; мозг; выступать; выдаваться" lex_translit="du">두</ana>
			</w>
			<w translit="sonbal">
				<ana lex="손발" gr="S" transl="руки и ноги; конечности; театральная кукла; марионетка" lex_translit="sonbal">손발</ana>
			</w>
			<w translit="i">
				<ana lex="이" gr="PART" transl="" lex_translit="i">이</ana>
			</w>
		</se>
		<se lang="rus">От того, что мы съели.
		</se>
	</para>
	<para id="9002">
		<se lang="kor" has_translit="true">
			<w translit="sarojabeun">
				<ana lex="사로잡다" gr="V,partcp" transl="захватить в плен; захватить; затуманить разум" lex_translit="sarojapda">사로잡은</ana>
			</w>
			<w translit="ireon">
				<ana lex="이런다" gr="A" transl="" lex_translit="ireonda">이런</ana>
			</w>
			<w translit="kkeulrim">
				<ana lex="끌리" gr="S" transl="задевать (одежду об пол); интересоваться; проявлять внимание; тащиться; волочиться; тянуться; вестись; быть ведомым; быть на поводу; повестись на; быть увлечённым; Вестись за руку; увлекаться за руку за кем-либо" lex_translit="kkeulri">끌림</ana>
			</w>
		</se>
		<se lang="rus">Неотвратимое притяжения,
		</se>
	</para>
	<para id="9003">
		<se lang="kor" has_translit="true">
			<w translit="ttel">
				<ana lex="떼다" gr="V,partcp" transl="отрывать, отделять, отлеплять" lex_translit="tteda">뗄</ana>
			</w>
			<w translit="suga">
				<ana lex="수가" gr="S" transl="; стоимость (каких-либо) услуг" lex_translit="suga">수가</ana>
			</w>
			<w translit="eopjana">
				<ana lex="없다" gr="A" transl="не быть, не иметь, отсутствовать" lex_translit="eopda">없잖아</ana>
			</w>
		</se>
		<se lang="rus">Ты не можешь отринуть его,
		</se>
	</para>
	<para id="9004">
		<se lang="kor" has_translit="true">
			<w translit="jom">
				<ana lex="좀" gr="S" transl="немного; немного; недолго; нисколько; ничуть; мелкий воришка; карманник; ничтожество; мелкий; маловажный; мелочный; ничтожный; пустяковый; незначительный; мелочный; низкий; узкий; ограниченный; антимоль; нафталин; нисколько; ничуть; мелкий мелочный человек" lex_translit="jom">좀</ana>
			</w>
			<w translit="deo">
				<ana lex="더" gr="S" transl="еще, более" lex_translit="deo">더</ana>
			</w>
			<w translit="gipi">
				<ana lex="깊이" gr="ADV" transl="глубоко; глубина; глубоко; важно; серьёзно; глубоко; серьёзно; значительно; сильно; глубоко" lex_translit="gipi">깊이</ana>
			</w>
			<w translit="ppajyeodeul">
				<ana lex="빠져들다" gr="V" transl="впасть; впасть; впадать; быть очарованным чем-либо" lex_translit="ppajyeodeulda">빠져들</ana>
			</w>
			<w translit="ttae">
				<ana lex="때" gr="S" transl="время, пора, момент; случай; сезон; грязь" lex_translit="ttae">때</ana>
			</w>
		</se>
		<se lang="rus">Когда ты с каждым мгновением все сильнее тонешь.
		</se>
	</para>
	<para id="9007">
		<se lang="kor" has_translit="true">
			<w translit="ui">
				<ana lex="의" gr="PART,gen" transl="" lex_translit="ui">의</ana>
			</w>
			<w translit="hyeokkeut">
				<ana lex="혀끝" gr="S" transl="; кончик языка" lex_translit="hyeokkeut">혀끝</ana>
			</w>
			<w translit="wiro">
				<ana lex="위로" gr="S" transl="утешение; компенсация; денежная премия; утешаться; утешать" lex_translit="wiro">위로</ana>
			</w>
			<w translit="areungeoril">
				<ana lex="아른거리다" gr="V,partcp" transl="мерцать; переливаться; пестрить" lex_translit="areungeorida">아른거릴</ana>
			</w>
		</se>
		<se lang="rus">Наслаждайся идеальным вкусом, что уже за пределами твоего языка.
		</se>
	</para>
	<para id="9008">
		<se lang="kor" has_translit="true">
			<w translit="wanbyeokhaejin">
				<ana lex="완벽해진" gr="S" transl="" lex_translit="wanbyeokhaejin">완벽해진</ana>
			</w>
			<w translit="mat">
				<ana lex="맛" gr="S" transl="вкус, настроение, атмосфера" lex_translit="mat">맛</ana>
			</w>
			<w translit="eul">
				<ana lex="을" gr="PART,acc" transl="" lex_translit="eul">을</ana>
			</w>
			<w translit="jeulgyeo">
				<ana lex="즐기다" gr="V" transl="наслаждаться, любить, веселиться" lex_translit="jeulgida">즐겨</ana>
			</w>
		</se>
		<se lang="rus">Ты желаешь этого, малышка!
		</se>
	</para></xml>