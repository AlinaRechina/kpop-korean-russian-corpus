<?xml version='1.0' encoding='utf-8'?>
<xml>
	<para id="1">
		<se lang="kor" has_translit="true">
			<w translit="neon">
				<ana lex="널다" gr="V,partcp" transl="развешивать, вешать" lex_translit="neolda">넌</ana>
			</w>
			<w translit="dareun">
				<ana lex="다른다" gr="A" transl="" lex_translit="dareunda">다른</ana>
			</w>
			<w translit="saenggakhal">
				<ana lex="생각다" gr="V,partcp" transl="" lex_translit="saenggakda">생각할</ana>
