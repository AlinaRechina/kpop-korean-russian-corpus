		<se lang="kor" has_translit="true">
			<w translit="neol">
				<ana lex="널" gr="S" transl="доска" lex_translit="neol">널</ana>
			</w>
			<w translit="deo">
				<ana lex="더" gr="S" transl="еще, более" lex_translit="deo">더</ana>
			</w>
			<w translit="joahan">
				<ana lex="좋아하다" gr="V,partcp" transl="любить, нравиться, обожать" lex_translit="joahada">좋아한</ana>
			</w>
			<w translit="geoji">
				<ana lex="거지" gr="S" transl="нищий" lex_translit="geoji">거지</ana>
			</w>
		</se>
