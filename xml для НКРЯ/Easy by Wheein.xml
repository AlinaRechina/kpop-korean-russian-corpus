		<se lang="kor" has_translit="true">
			<w translit="ege">
				<ana lex="에게" gr="PART" transl="" lex_translit="ege">에게</ana>
			</w>
			<w translit="gadwodun">
				<ana lex="가두다" gr="V,partcp" transl="запирать; держать в заперти; запастись; набрать про запас" lex_translit="gaduda">가둬둔</ana>
